<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.punuo.sys.app.imgdot">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.ImageBrowseActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.FdomainActivity" />
        <activity android:name=".activity.GdomainActivity" />
        <activity android:name=".activity.HdomainActivity" />
        <activity android:name=".activity.VideoPlayActivity" />
        <activity android:name=".activity.RecordPlayActivity"></activity>
    </application>

</manifest>